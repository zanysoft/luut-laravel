{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-zip": "*",
    "doctrine/dbal": "^4.1",
    "geo-sot/laravel-env-editor": "^2.1",
    "google/apiclient": "^2.17",
    "inertiajs/inertia-laravel": "^1.0",
    "intervention/image": "^3.8",
    "jackiedo/dotenv-editor": "*",
    "laravel/framework": "^11.9",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^5.16",
    "laravel/tinker": "^2.9",
    "laravel/ui": "^4.5",
    "league/omnipay": "^3.2",
    "omnipay/stripe": "^3.2",
    "opcodesio/log-viewer": "^3.11",
    "prologue/alerts": "^1.2",
    "spatie/image": "^3.7",
    "spatie/laravel-mail-preview": "^6.2",
    "spatie/laravel-permission": "^6.9",
    "spatie/laravel-sitemap": "^7.2",
    "stripe/stripe-php": "^16.1",
    "tightenco/ziggy": "^2.0",
    "yajra/laravel-datatables": "^11.0",
    "zanysoft/laravel-responsive-filemanager": "^3.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "knuckleswtf/scribe": "^4.37",
    "laravel/breeze": "^2.2",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0.1",
    "spatie/laravel-ignition": "^2.4"
  },
  "autoload": {
    "files": [
      "app/Helpers/admin.php",
      "app/Helpers/core.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
